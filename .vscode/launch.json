{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch server.ts",
        "type": "node",
        "request": "launch",
        "runtimeArgs": [
            "-r",
            "ts-node/register",
            "-r", 
            "tsconfig-paths/register"
